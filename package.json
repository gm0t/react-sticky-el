{
  "name": "react-sticky-el",
  "version": "1.1.0",
  "description": "Sticky component for React",
  "browserslist": "> 0.25%, not dead",
  "main": "lib/index.js",
  "jsnext:main": "es/index",
  "scripts": {
    "build": "npm run build-cjs && npm run build-es && npm run build-dist",
    "build-es": "rimraf es && cross-env BABEL_ENV=es babel src --out-dir es",
    "build-cjs": "rimraf lib && cross-env BABEL_ENV=cjs babel ./src -d lib --ignore __tests__",
    "build-dist": "rimraf dist && npm run build-dist-examples && npm run build-dist-lib",
    "build-dist-examples": "NODE_ENV=production webpack -p --config webpack-examples.config.js",
    "build-dist-lib": "webpack src/index.js -o dist/react-sticky-el.js && NODE_ENV=production webpack -p src/index.js -o dist/react-sticky-el.min.js --display-modules --progress",
    "start": "webpack-dev-server --config webpack-examples.config.js",
    "test": "BABEL_ENV=test mocha --require babel-core/register --require ./test/setup test/unit",
    "test-watch": "BABEL_ENV=test mocha -w --require babel-core/register --require ./test/setup test/unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gm0t/react-sticky-el.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "sticky",
    "fixed",
    "react-sticky"
  ],
  "files": [
    "lib",
    "es",
    "*.md"
  ],
  "author": "Mikhail Bogdanov <admin@gm0.ru> (https://github.com/gm0t)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gm0t/react-sticky-el/issues"
  },
  "homepage": "https://github.com/gm0t/react-sticky-el#readme",
  "peerDependencies": {
    "react": ">=16.3.0",
    "react-dom": ">=16.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@types/react": "^16.9.19",
    "ajv": "^6.11.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-plugin-rewire-exports": "^2.1.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "fsevents": "^2.1.2",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^16.1.0",
    "mocha": "^7.0.1",
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.19",
    "rimraf": "^3.0.1",
    "sass-loader": "^8.0.2",
    "sinon": "^8.1.1",
    "sinon-chai": "^3.4.0",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.10.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "prop-types": ">=15.7.2"
  }
}
